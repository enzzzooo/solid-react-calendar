{
  "name": "solid-react-calendar",
  "version": "1.1.31",
  "description": "A well-designed solid react calendar",
  "keywords": [
    "calendar",
    "react",
    "nextjs",
    "date",
    "month",
    "tailwind"
  ],
  "license": "MIT",
  "author": "enzzzooo",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enzzzooo/solid-react-calendar.git"
  },
  "homepage": "https://github.com/enzzzooo/solid-react-calendar",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && rollup -c ",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^20",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@types/react-window": "^1.8.8",
    "autoprefixer": "^10.4.20",
    "eslint": "^9",
    "postcss": "^8.5.1",
    "postcss-import": "^16.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rollup": "^3.29.4",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-preserve-directives": "^0.4.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "react-window": "^1.8.11"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/es/index.d.ts",
        "default": "./dist/es/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./DatePicker": {
      "import": {
        "types": "./dist/es/DatePicker.d.ts",
        "default": "./dist/es/DatePicker.js"
      },
      "require": {
        "types": "./dist/cjs/DatePicker.d.cts",
        "default": "./dist/cjs/DatePicker.cjs"
      }
    },
    "./Calendar": {
      "import": {
        "types": "./dist/es/Calendar.d.ts",
        "default": "./dist/es/Calendar.js"
      },
      "require": {
        "types": "./dist/cjs/Calendar.d.cts",
        "default": "./dist/cjs/Calendar.cjs"
      }
    }
  }
}
