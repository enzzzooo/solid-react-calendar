{
  "name": "solid-react-calendar",
  "version": "1.1.29",
  "description": "A well-designed solid react calendar",
  "keywords": [
    "calendar",
    "react",
    "nextjs",
    "date",
    "month",
    "tailwind"
  ],
  "license": "MIT",
  "author": "enzzzooo",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enzzzooo/solid-react-calendar.git"
  },
  "homepage": "https://github.com/enzzzooo/solid-react-calendar",
  "type": "module",
  "main": "./dist/index.js",
  "files": [
    "./dist",
    "dist"
  ],
  "types": "./dist/es/index.d.ts",
  "scripts": {
    "build:css": "tailwindcss -i ./src/main.css -o ./dist/index.css --minify",
    "build": "bunchee && npm run build:css",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-window": "^1.8.8",
    "autoprefixer": "^10.4.20",
    "bunchee": "^6.3.2",
    "eslint": "^9",
    "postcss": "^8.5.1",
    "postcss-import": "^16.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "react-window": "^1.8.11"
  },
  "module": "./dist/es/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/es/index.d.ts",
        "default": "./dist/es/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./DatePicker": {
      "import": {
        "types": "./dist/es/DatePicker.d.ts",
        "default": "./dist/es/DatePicker.js"
      },
      "require": {
        "types": "./dist/cjs/DatePicker.d.cts",
        "default": "./dist/cjs/DatePicker.cjs"
      }
    },
    "./Calendar": {
      "import": {
        "types": "./dist/es/Calendar.d.ts",
        "default": "./dist/es/Calendar.js"
      },
      "require": {
        "types": "./dist/cjs/Calendar.d.cts",
        "default": "./dist/cjs/Calendar.cjs"
      }
    }
  }
}
